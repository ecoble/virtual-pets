<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<fx:root type="javafx.scene.layout.StackPane" fx:id="stackPane" xmlns:fx="http://javafx.com/fxml/1" prefHeight="300.0" prefWidth="500.0">
    <ImageView fx:id="backgroundImage" fitWidth="650" preserveRatio="true">
        <image>
            <Image url="@../../images/park.jpg" />
        </image>
    </ImageView>
    <Group>
        <HBox fx:id="birdContainer" StackPane.alignment="TOP_CENTER">
        </HBox>
    </Group>
    <Group StackPane.alignment="BOTTOM_CENTER">
        <HBox fx:id="petContainer">
        </HBox>
    </Group>
</fx:root>